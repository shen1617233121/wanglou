
 
class Parent:       
   stuCount = 0

   def __init__(self,name,stu_no,class_no,gender):
       self.name = name
       self.stu_no = stu_no
       self.class_no = gender
       stuCount += 1
   def study(self):
       print "Student can study"
      
   def ParentMethod(self):
       return Student.StuCount


class PrimaryStudent(Student):
   primaryStuCount = 0

   def canRecite(self):
       print"Prinary Student can recite"
 
   def canOral(self):
       print"Prinary Student can oral"


 
class MiddleStudent(Student):
   middleStuCount = 0

    def __init__(self,name,stu_no,class_no,gender):
       self.name = name
       self.stu_no = stu_no
       self.class_no = class_no
       self.gender = gender
       Student.stuCount += 1
       MiddleStudent.middleStuCount +=1
       
   def canChenistry(self)
       print "middle Student can Chemistry"
 
   def canPyhics(self):
       print "middle Student can Physice"
 
